---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Gosha Arinich</title>
 <link href="http://goshakkk.name/atom.xml" rel="self"/>
 <link href="http://goshakkk.name/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://goshakkk.nameid>
 <author>
   <name>Gosha Arinich</name>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://goshakkk.name{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://goshakkk.name{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>